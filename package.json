{
  "name": "outright-react-2024",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate-client-work-page-index": "node app/our-work/client-work-pages/scripts/generateIndex.mjs",
    "generate-bg-zoom-img-index": "node app/zoom/scripts/generateBgZoomImageIndex.mjs",
    "generate-outright-zoom-img-index": "node app/zoom/scripts/generateOutrightZoomImageIndex.mjs",
    "generate-team-headshot-index": "node app/team/scripts/generateTeamImgIndex.mjs",
    "generate-all-indexes": "npm run generate-client-work-page-index && npm run generate-bg-zoom-img-index && npm run generate-outright-zoom-img-index && npm run generate-team-headshot-index",
    "dev": "npm run generate-all-indexes && next dev",
    "build": "npm run generate-all-indexes && next build && npm run postbuild",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test-debug": "vitest --debug",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@vitejs/plugin-react": "^4.3.3",
    "bootstrap": "^5.3.3",
    "framer-motion": "^11.11.10",
    "fullpage.js": "^4.0.29",
    "next": "14.2.13",
    "next-sitemap": "^4.2.3",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^10.0.6",
    "react": "^18",
    "react-dom": "^18",
    "react-iframe": "^1.8.5",
    "react-scroll-parallax": "^3.4.5",
    "sass": "^1.79.4",
    "scss": "^0.2.4",
    "uglify-es": "^3.3.10",
    "uglifyjs": "^2.4.11",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@vitest/ui": "^2.1.4",
    "eslint": "^8",
    "eslint-config-next": "14.2.13",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.1",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
